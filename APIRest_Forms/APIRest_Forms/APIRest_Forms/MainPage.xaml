             xmlns:local="clr-namespace:APIRest_Forms"
             Title="Consuming API Rest X#"
             x:Class="APIRest_Forms.MainPage">
    <StackLayout>
      
        <ListView x:Name="MyListView">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding Title}" TextColor="Black"/>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>
